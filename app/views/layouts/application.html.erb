<!DOCTYPE html>
<html>
<head>
  <title>ReTrack</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

  <% link_to 'Main', :root %>
  <% if current_user %>
    <%= link_to 'Logout', :logout %>
  <% else %>
    <%= link_to 'Login', :login %>
    <%= link_to 'Register', new_user_path %>
  <% end %>

<%= yield %>

</body>
</html>
